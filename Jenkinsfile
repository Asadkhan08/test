pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'This is building stage'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'This is testing  stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'This is deploying stage'
            }
        }
    }
}
